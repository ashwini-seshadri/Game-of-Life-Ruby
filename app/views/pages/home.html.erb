<h1>Pages->Home</h1>
<p>Location: app->views->pages->home</p>
<%= content_tag(:div, nil, data: {
  controller: "life",
}) do %>
  <%= content_tag(:canvas, nil, data: {
    life_target: "canvas"
  }) %>
  <br />
  <%= button_tag "Start", data: { action: "click->life#start" }, id: "start_button" %>
  <%= button_tag "Clear", data: { action: "click->life#clear" } %>
  <%= button_tag "Random", data: { action: "click->life#random" } %>
<% end %>